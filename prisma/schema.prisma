// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
  output = "./client"
}

datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

model Repo {
    id   String     @id @default(uuid())
    url String
    name  String?
    user  String?
    github_data String
    topics String
    inodes String
    fullUrl String
    dependents String
    setup String
    config String?
}